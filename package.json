{
  "name": "slate-sync-bridge",
  "author": {
    "name": "Alex Hixon",
    "email": "alex@alexhixon.com"
  },
  "version": "0.1.0",
  "private": true,
  "license": "UNLICENSED",
  "dependencies": {
    "@types/slate": "^0.44.8",
    "fast-deep-equal": "^2.0.1",
    "immutable": "^4.0.0-rc.12",
    "slate": "^0.47.3",
    "typescript": "3.5.1"
  },
  "scripts": {
    "test": "yarn jest"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}",
      "<rootDir>/test/**!(slate)/*.{js,jsx,ts,tsx}",
      "<rootDir>/test/*.{js,jsx,ts,tsx}",
      "<rootDir>/test/slate/index.js"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!slate-dev-test-utils).+\\.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "24.0.13",
    "@types/node": "12.0.4",
    "@typescript-eslint/eslint-plugin": "1.6.0",
    "@typescript-eslint/parser": "1.6.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "^24.8.0",
    "eslint": "^5.16.0",
    "jest": "^24.8.0",
    "react": "^16.8.6",
    "slate-hyperscript": "^0.13.3"
  }
}
